<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>ozwcp</Name>
  <Overview>Open Zwave Control Panel</Overview>
<Support></Support>
<Description>Open Zwave Control Panel</Description>
  <Registry>https://registry.hub.docker.com/u/pscherrer80/ozwcp/</Registry>
  <Repository>pscherrer80/ozwcp</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
       <Port> 
         <HostPort>8090</HostPort> 
         <ContainerPort>8090</ContainerPort> 
         <Protocol>tcp</Protocol> 
       </Port> 
     </Publish> 
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/ozwcp/</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version></Version>
  <WebUI>http://[IP]:[PORT:8090]</WebUI>
  <Banner></Banner>
  <Icon></Icon>
</Container>
